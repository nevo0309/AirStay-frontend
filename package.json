{
  "name": "vite-react-proj",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "sass-dev": "sass --watch src/assets/styles/main.scss:src/assets/css/main.css --style=expanded --quiet-deps",
    "sass-prod": "sass src/assets/styles/main.scss:src/assets/css/main.css --no-source-map --style=compressed",
    "dev": "vite",
    "dev:local": "set VITE_LOCAL=true && vite",
    "dev:local:mac": "export VITE_LOCAL=true && vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "embla-carousel-react": "^8.6.0",
    "embla-carousel-reactive-utils": "^8.6.0",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.1",
    "react-router-dom": "^7.6.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "eslint": "^9.27.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "sass": "^1.89.0",
    "vite": "^6.3.5"
  }
}
